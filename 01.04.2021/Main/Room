public class Room {
    int place;
    boolean shower;
    boolean toilet;
    boolean wifi;
    boolean eat;
    int roomNumber;

    public Room(int place, boolean shower, boolean toilet, boolean wifi, boolean eat, int roomNumber) {
        this.place = place;
        this.shower = shower;
        this.toilet = toilet;
        this.wifi = wifi;
        this.eat = eat;
        this.roomNumber = roomNumber;
    }
}
